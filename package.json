{
  "name": "aft-web-services",
  "version": "3.0.0",
  "description": "Automated Functional Testing (AFT) module for testing web services over HTTP and HTTPS",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --declaration",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "web-services",
    "rest"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bicarbon8/aft-web-services/issues"
  },
  "homepage": "https://github.com/bicarbon8/aft-web-services#readme",
  "dependencies": {
    "aft-core": "^3.1.0",
    "es6-promise": "^4.2.8",
    "xmldom": "^0.4.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.3",
    "@types/node": "^12.19.15",
    "@types/node-uuid": "0.0.28",
    "@types/xmldom": "^0.1.30",
    "jasmine": "^3.6.4",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
